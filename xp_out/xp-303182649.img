var i;
var strSrc;
var strDoc;
if (nFrames > 0)
this._fFrameset = true;
for (i = 0; i < nFrames; i++)
{
strSrc = aFrames[i].src;
if (strSrc == "res://SHDOCLC.DLL/printnof.htm")
continue;
strDoc = this.CreateSubDocument(strSrc, true);
if (i == nActive)
g_strActiveFrame = strDoc;
g_nFramesLeft++;
OnBuildAllFrames(strDoc);
}
g_nFramesLeft--;
