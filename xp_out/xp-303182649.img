}
}
function OnBuildAllFrames( strDoc )
{
if ( !g_aDocTree[strDoc]
|| !g_aDocTree[strDoc]._aaRect[1][0]
|| !g_aDocTree[strDoc]._aaRect[1][0].contentDocument.body)
{
window.setTimeout("OnBuildAllFrames('" + strDoc + "');", 100);
