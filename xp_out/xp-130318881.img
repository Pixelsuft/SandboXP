var L_YOURCLIPBOARD_TEXT = "Your clipboard text will appear here";

var L_CLOSETAB_EXPAND_TEXT = "expand closed tabs section";
var L_CLOSETAB_COLLAPSE_TEXT = "collapse closed tabs section";

var L_WEBSERVICE_EXPAND_TEXT = "expand web service section";
var L_WEBSERVICE_COLLAPSE_TEXT = "collapse web service section";

// Unlocalized strings
var SZ_COLLAPSE_IMG      = "collapse_nor.png";
var SZ_COLLAPSE_HVR_IMG  = "collapse_hvr.png";
var SZ_EXPAND_IMG        = "expand_nor.png";
var SZ_EXPAND_HVR_IMG    = "expand_hvr.png";

var SZ_CLOSE_IMG         = "close_nor.png";
var SZ_CLOSE_HVR_IMG     = "close_hvr.png";


var imgSrc = null;

function showhide(img, tObj)
{
    var state = tObj.style.display;

    var fVisible = false;

    if (state == '' || state == "none")
    {
        tObj.style.display = "block";
        img.src = SZ_COLLAPSE_IMG;
        if (img == tabglyph)
        {
            img.alt = L_CLOSETAB_COLLAPSE_TEXT;
        }
        else if (img == SIglyph)
        {
            img.alt = L_WEBSERVICE_COLLAPSE_TEXT;
        }
        fVisible = true;
    } 
    else 
    {
        tObj.style.display = "none";
        img.src = SZ_EXPAND_IMG;
        if (img == tabglyph)
        {
            img.alt = L_CLOSETAB_EXPAND_TEXT;
        }
        else if (img == SIglyph)
        {
            img.alt = L_WEBSERVICE_EXPAND_TEXT;
        }
    }

    imgSrc = img.src;

    return fVisible;
}

function showhideClipboard(showClick, clipboard, clipboardTextStatsContainer, useThisTextContainer, clipboardText, clipboardTextStats, fShow)
{  
    clipboardTextStatsContainer.style.display = "none"; // the clipboard character count text

    if (fShow)
    {
        clipboard.style.display = "block";  // the clipboard container
        clipboardText.style.display = "inline";  // the clipboard text, need to set explicitly (even though it's container is getting set)
                                                 // so the clipboard retrieval handler can know if it is displaying or not
        
        showClick.style.display = "none";
        
        var text = clipboardText.innerText;
        if (text == null || text.length == 0 || text == L_YOURCLIPBOARD_TEXT)
        {
            // clipboard text is empty
            clipboardText.innerText = L_YOURCLIPBOARD_TEXT;
            
            // do not show "Use this text: " to the user if their clipboard is empty.
            useThisTextContainer.style.display = "none";
        }
        else
        {
            // clipboard.innerText will have been set by CClipboardRetrievalHandler
            useThisTextContainer.style.display = "inline";
            
            var statsTextShowing = clipboardTextStats.innerText;
            
            if (statsTextShowing != null && statsTextShowing.length != 0) 
            {
                clipboardTextStatsContainer.style.display = "block";
            }
        }
    }
    else
    {
        clipboard.style.display = "none";
        clipboardText.style.display = "none";
        
        showClick.style.display = "block";
    }
    
    return false; // suppress default action
}

